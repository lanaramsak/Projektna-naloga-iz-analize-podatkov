        e.preventDefault();

        $("#get-info .kontakt").dialog({
            modal: true,
            title: 'Kras nepremičnine, Nina Čebulec s.p.',
            cache: false,
            resizable: false,
            close: function(event, ui) {
                $(this).dialog("destroy");
            },
