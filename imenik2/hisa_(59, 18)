    $('.contact_mobile, .contact_mobile a').on('click', function (e) {
        e.preventDefault();

        $("#get-info .kontakt").dialog({
            modal: true,
            title: 'Kristal nepremičnine d.o.o.',
            cache: false,
            resizable: false,
            close: function(event, ui) {
                $(this).dialog("destroy");
