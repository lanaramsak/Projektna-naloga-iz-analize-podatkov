            jQuery(document).ready(function($) {
                $('#galerija.royalSlider').royalSlider({
                    fullscreen: {
                        enabled: true,
                        buttonFS: true,
                        nativeFS: false
                    },
                    controlNavigation: 'thumbnails',
                    autoScaleSlider: true,
                    autoScaleSliderWidth: 320,
                    autoScaleSliderHeight: 271,
                    loop: false,
                    //imageScaleMode: 'fit-if-smaller',
                    imageScaleMode: 'fit',
                    navigateByClick: true,
                    numImagesToPreload:2,
                    arrowsNav: true,
                    arrowsNavAutoHide: false,
                    arrowsNavHideOnTouch: false,
                    keyboardNavEnabled: false,
                    fadeinLoadedSlide: false,
                    globalCaption: true,
                    globalCaptionInside: true,
                    thumbs: {
                        autoCenter: true,
                        arrowsAutoHide: false,
                        appendSpan: false,
                        firstMargin: 23,
                        paddingBottom: 4
                    },
                    video: {
                        // video options go gere
                        autoHideBlocks: true,
                        autoHideArrows: false,
                        youTubeCode: '<iframe src="https://www.youtube.com/embed/%id%?rel=0&autoplay=1&showinfo=0?enablejsapi&wmode=transparent" frameborder="no"></iframe>'
                    }
                });

                var slider = $(".royalSlider").data('royalSlider');
                slider.ev.on('rsEnterFullscreen', function() {
                    $('#bar-top-wrapper').css('z-index', 0);
                    $('.contact_mobile').css('z-index', 380);
                });
                slider.ev.on('rsExitFullscreen', function() {
                    $('#bar-top-wrapper').css('z-index', 401);
                    $('.contact_mobile').css('z-index', 410);
                });

                // noTrans
                var s = $('.royalSlider').data('royalSlider');
                s.st.transitionSpeed = 0;
                setTimeout(function() {
                        s.st.transitionSpeed = 0;
                });

                // sliderCount
                if (mobileAndTabletcheck()) {
                    var sliderJQ = $('.royalSlider').royalSlider();
                    var sliderInstance = sliderJQ.data('royalSlider');
                    var slideCountEl = $('<div class="rsSlideCount"></div>').appendTo( sliderJQ );

                    function updCount() {
                        slideCountEl.html( (sliderInstance.currSlideId+1) + ' od ' + sliderInstance.numSlides );
                    }
                    sliderInstance.ev.on('rsAfterSlideChange', updCount);
                    updCount();
                }

                $(document).keydown(function(e) {
                    switch(e.which) {
                        case 37: // left
                            if (!e.altKey) {
                                $('.royalSlider').data('royalSlider').prev();
                                e.preventDefault(); // prevent the default action (scroll / move caret)
                            }
                            break;

                        case 39: // right
                            if (!e.altKey) {
                                $('.royalSlider').data('royalSlider').next();
                                e.preventDefault(); // prevent the default action (scroll / move caret)
                            }
                            break;
                    }
                });

            });
