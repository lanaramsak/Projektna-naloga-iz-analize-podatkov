    $(document).ready(function(e){
        $('.contact-tooltip').tooltipster({
            theme: 'tooltipster-shadow',
            contentAsHTML: true,
            position: 'left',
            interactive: true
        });

        if (mobileAndTabletcheck()){
            $('.contact-tooltip').click(function(){
                return false;
            });
        }

        //add possibility do display mobiel version
        if (mobileAndTabletcheck() && $('#logo-m').is(':hidden') && $(".scrsz").width() < 1043) {
            if (document.documentElement.lang == 'sl')
                var switchText = 'PREKLOPI NA MOBILNO VERZIJO';
            else
                var switchText = 'MOBILE VERSION';
            $('#all').after('<div class="use-mobile-version"><a href="?mobile">'+switchText+'</a></div>');
        }
    });
