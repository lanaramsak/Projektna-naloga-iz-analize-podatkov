    $('.contact_mobile, .contact_mobile a').on('click', function (e) {
        e.preventDefault();

        $("#get-info .kontakt").dialog({
            modal: true,
            title: 'QUDOS, center nepremičnin d.o.o.',
            cache: false,
            resizable: false,
            close: function(event, ui) {
                $(this).dialog("destroy");
            },
            buttons: {
                'Confirm': {
                        click: function () {
                            var doc = $(".inquiry-form").find("button");
                            doc.click();
                        },
                        disabled: true,
                        text: 'Pošlji sporočilo',
                        class: 'ui-ok'
                    },
                "Cancel": {
                        click: function () {
                            $(this).dialog("destroy");
                        },
                        text: 'Zapri',
                        class: 'ui-cancle'
                    }
              }
        });
    });
