                        disabled: true,
                        text: 'Pošlji sporočilo',
                        class: 'ui-ok'
                    },
                "Cancel": {
                        click: function () {
                            $(this).dialog("destroy");
                        },
                        text: 'Zapri',
                        class: 'ui-cancle'
                    }
              }
        });
    });
