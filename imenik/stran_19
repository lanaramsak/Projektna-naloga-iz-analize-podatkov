                            }
                        });
                            /*Add generate url for custom Leto*/
                            $('.more.year input[type=submit]').on('click', function(e) {
                                e.preventDefault();
                                var regex = /&l1=(.*?)[^;]*/igm;
                                filter_url = filter_url.replace(regex, '');

                                var val_from = $('.more.year input[name=l1]').val();
